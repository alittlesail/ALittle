    // 确定设置过滤
    private fun HandleLogFliterConfirmClick(ALittle.UIEvent event)
    {
        this._log_fliter_dialog.visible = false;
        var fliter_list = ALittle.String_SplitSepList(this._log_fliter_edit.text, ["\r","\n"]);
