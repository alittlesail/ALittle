    private ALittle.DisplayObject _plugin_file_input;

    // 标签页
    private ALittle.Tab _main_tab;

    private ALittle.Dialog _log_fliter_dialog;
    private ALittle.ImageEdit _log_fliter_edit;
    private Map<string, bool> _fliter_map;

    // 帧循环
