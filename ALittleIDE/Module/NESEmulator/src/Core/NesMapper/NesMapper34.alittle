
namespace NESEmulator;

protected class NesMapper34 : NesMapper0
{
    public fun Write(int address, int value)
    {
        if (address < 0x8000) {
            NesMapper0.Write(this, address, value);
            return;
        } else {
            this.Load32kRomBank(value, 0x8000);
        }
    }
}
