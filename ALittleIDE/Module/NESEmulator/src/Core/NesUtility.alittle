
namespace NESEmulator;

protected static ConditionExpr(bool value, int a, int b) : int
{
    if (value) return a;
    return b;
}

protected static CopyArrayElements<T>(Map<int, T> src, int srcPos, Map<int, T> dest, int destPos, int length)
{
	for (var i = 0; i < length; 1) dest[destPos + i] = src[srcPos + i];
}